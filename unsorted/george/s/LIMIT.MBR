00010 REM    FIND SIN(X)/X AS X --> 0
00011 PRINT #255:"THIS PROGRAM FINDS THE LIMIT OF SIN(X)/X AS X APPROACHES 0."
00012 OPTION LPREC
00015 PRINT #255: "   X";TAB(12);"F(X)";TAB(29);"-X";TAB(39);"F(-X)"
00020 FOR I=1 TO 1E-10 STEP -.02
00030 PRINT #255: I;TAB(10);SIN(I)/I;TAB(30);-I;TAB(40);SIN(-I)/(-I)
00040 NEXT I
00050 END
